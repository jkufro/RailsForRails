<% type ||= 'text' %>
<% label ||= vmodel %>

<div class='row'>
    <div class='col s8 offset-s2'>
        <div class="input-field col s12">
            <input id="<%= id %>" type="<%= type %>" v-model="<%= vmodel %>">
            <label for="<%= id %>"><%= label %></label>
        </div>
    </div>
</div>
