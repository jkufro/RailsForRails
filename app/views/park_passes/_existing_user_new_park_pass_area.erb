<div class="row" id='existing_user_new_park_pass_area'>
    <div class="col s12">
        <div class='row no-margin-bottom'>
            <div class='col s12'>
                <h4>Add Park Pass</h4>
            </div>
        </div>
        <div class='row no-margin-bottom no-margin-top'>
            <div class='col s12'>
                <%= render partial: 'park_passes/park_pass_form', locals: { :id_prefix => 'existing_user' } %>
            </div>
        </div>

        <div class='row'>
            <div class='col s12'>
                <a class="waves-effect waves-light btn center" v-on:click="create_park_pass()">
                    <i class="material-icons left">create</i>
                    Add Park Pass
                </a>
            </div>
        </div>
    </div>
</div>
